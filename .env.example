# Firebase/GCP Configuration Template
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit the actual .env file to version control

# GCP Configuration
GCP_PROJECT_ID=gcp-f06-barcode
GCP_PROJECT_NUMBER=your_project_number_here
GCP_REGION=asia-northeast1

# Firebase Configuration (Required)
# Get these values from Firebase Console > Project Settings > General
FIREBASE_API_KEY=AIzaSy_your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=gcp-f06-barcode
FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
FIREBASE_APP_ID=1:your_sender_id:web:your_app_id_here

# Vertex AI Configuration
VERTEX_AI_PROJECT=gcp-f06-barcode
VERTEX_AI_LOCATION=asia-northeast1
VERTEX_AI_ENDPOINT=asia-northeast1-aiplatform.googleapis.com

# API Keys (REQUIRED - Get from respective services)
# Gemini API Key - Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=AIzaSy_your_gemini_api_key_here

# FCM Web Push (Optional for web notifications)
# Get from Firebase Console > Project Settings > Cloud Messaging
VAPID_KEY=your_vapid_key_here

# Open Food Facts API
OPEN_FOOD_FACTS_USER_AGENT=FridgeManager/1.0

# Google Sign-In Configuration
# Get from Google Cloud Console > APIs & Services > Credentials
GOOGLE_SIGNIN_IOS_CLIENT_ID=your_ios_client_id.apps.googleusercontent.com
GOOGLE_SIGNIN_WEB_CLIENT_ID=your_web_client_id.apps.googleusercontent.com

# Environment Settings
ENVIRONMENT=development

# Security Settings (Optional)
SECURITY_ENCRYPTION_ENABLED=true
SECURITY_BIOMETRIC_ENABLED=true
SECURITY_API_KEY_ROTATION_ENABLED=true

# Debug Settings (Development Only)
DEBUG_LOGGING_ENABLED=true
DEBUG_MOCK_API_RESPONSES=false